{
  "name": "tester-names",
  "version": "0.0.0-development",
  "description": "find tester names",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "check-coverage": "istanbul check-coverage --statement 100 --branches 100 --functions 100 --lines 100",
    "test": "mocha src/index.test.js -w",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avif444/tester-names.git"
  },
  "keywords": [
    "random",
    "tester",
    "name"
  ],
  "author": "avif muhamad taufiq <avifmuhamadtaufiq4@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/avif444/tester-names/issues"
  },
  "homepage": "https://github.com/avif444/tester-names#readme",
  "dependencies": {
    "ghooks": "2.0.2",
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "istanbul": "0.4.5",
    "mocha": "5.0.0",
    "semantic-release": "^12.4.1",
    "travis-deploy-once": "^4.3.4"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
